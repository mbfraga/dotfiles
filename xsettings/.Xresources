Xcursor.size: 14

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: grayscale
Xft.dpi: 96


URxvt*termName: rxvt-unicode-256color
! Color Defintions
!

