run:
act(t, terminal, st)
act(f, browser, open_if_noinst firefox Firefox)
act(s, speedcrunch, open_if_noinst speedcrunch SpeedCrunch)
act(g, steam, open_if_noinst steam Steam)
act(e, file manager, dolphin)
act(m, mbmain, mbmain)
act(q, qnr, qnr)
act(c, telegram, open_if_noinst telegram TelegramDesktop)
act(w, weechat, open_if_noinst  "st -c weechat-st -e weechat -d $XDG_CONFIG_HOME/weechat" weechat-st)
act(n, ncmcpcpp, open_if_noinst  "st -c ncmpcpp-st -e ncmpcpp" ncmpcpp-st)
